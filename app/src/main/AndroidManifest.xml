<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="android.com.myapplication">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="com.android.MainActivity"
            android:label="@string/title_activity_main"></activity>
        <activity android:name="com.android.Sub1Activity" />
        <activity android:name="com.android.Sub3Activity" />
        <activity android:name="com.android.Sub4Activity" />
        <activity android:name="com.android.BootPageActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.android.Sub2Activity"></activity>
    </application>

</manifest>